RewriteCond %{HTTP_HOST} !^www\. [NC]
RewriteRule ^(.*)$ http://www.%{HTTP_HOST}/$1 [L,R=301]

RewriteCond %{HTTP_HOST} woopsgram\.com$
RewriteRule ^(.*)$ http://www.whoopsgram.com/$1 [L,R=301]
