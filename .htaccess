RewriteEngine On

RewriteCond %{HTTP_HOST} !^www\. [NC]
RewriteRule ^(.*)$ http://www.%{HTTP_HOST}/$1 [R=301,L]

RewriteCond %{HTTP_HOST} woopsgram\.com$ RewriteCond %{QUERY_STRING} .+
RewriteRule (.*) http://www.whoopsgram.com$1?%{QUERY_STRING} [L,R=301,QSA]

RewriteCond %{HTTP_HOST} woopsgram\.com$
RewriteRule (.*) http://www.whoopsgram.com$1 [L,R=301,QSA]
